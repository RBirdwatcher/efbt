<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:functionality_module="http://www.eclipse.org/functionality_module" xmlns:report_based_report_based_functional_domain_breakdown="http://www.eclipse.org/report_based_report_based_functional_domain_breakdown" xmlns:report_structures="http://www.eclipse.org/report_structures" xmlns:xsd="http://www.w3.org/2001/XMLSchema" ecore:nsPrefix="report_based_report_based_functional_domain_breakdown" ecore:package="org.eclipse.efbt.chirp.report_based_report_based_functional_domain_breakdown" targetNamespace="http://www.eclipse.org/report_based_report_based_functional_domain_breakdown">
  <xsd:import namespace="http://www.eclipse.org/report_structures" schemaLocation="report_structures.xsd"/>
  <xsd:import namespace="http://www.eclipse.org/functionality_module" schemaLocation="functionality_module.xsd"/>
  <xsd:element ecore:ignore="true" name="ReportBasedFunctionalityModuleModule" type="report_based_report_based_functional_domain_breakdown:ReportBasedFunctionalityModuleModule"/>
