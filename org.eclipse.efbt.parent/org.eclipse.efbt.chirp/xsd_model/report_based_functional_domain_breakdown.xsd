  </xsd:complexType>
  <xsd:complexType name="ReportBasedSerialFunctionalityModule">
    <xsd:complexContent>
      <xsd:extension base="functionality_module:SerialFunctionalityModule"/>
    </xsd:complexContent>
  </xsd:complexType>
