  <xsd:element ecore:ignore="true" name="ReportBasedSerialFunctionalityModule" type="report_based_report_based_functional_domain_breakdown:ReportBasedSerialFunctionalityModule"/>
  <xsd:complexType name="ReportCellCreationFunctionalityModule">
