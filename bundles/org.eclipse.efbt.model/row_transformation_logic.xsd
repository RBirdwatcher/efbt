SPDX-License-Identifier: EPL-2.0 

 Contributors:
 Neil Mackenzie - initial API and implementation&#13;</xsd:appinfo>
  </xsd:annotation>
  <xsd:complexType name="BaseRowStructure">
    <xsd:annotation>
      <xsd:documentation>A set of CubeColumns which make up the structure of a BaseCube row</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element ecore:resolveProxies="true" maxOccurs="unbounded" minOccurs="0" name="columns" type="column_transformation_logic:CubeColumn">
        <xsd:annotation>
          <xsd:documentation>The set of CubeColumns</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute ecore:reference="cubes:BaseCube" name="cube" type="xsd:anyURI">
      <xsd:annotation>
        <xsd:documentation>The associated BaseCube</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute ecore:unsettable="false" name="usedInSubsets" type="ecore:EBoolean">
      <xsd:annotation>
        <xsd:documentation>The CompareAttributeLineageModels Platform call in the platform_call package creates an AttributeLineageModel where items (such as BaseRowStructures) which are in both models are marked with usedInSubset=true</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="ColumnFunctionGroup">
    <xsd:annotation>
      <xsd:documentation>A group  of ColumnFunctions</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element ecore:resolveProxies="true" maxOccurs="unbounded" minOccurs="0" name="columnFunctions" type="column_transformation_logic:ColumnFunction">
        <xsd:annotation>
          <xsd:documentation>The group  of ColumnFunctions</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="FunctionalRowLogic">
    <xsd:annotation>
      <xsd:documentation>FunctionalRowLogic represents contains a set of ColumnFunctions, each one of these Column Functions will be a function
    of Columns in BaseSchemas or other Column functions contained in other FunctionalRowLogics.  </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element ecore:resolveProxies="true" minOccurs="0" name="columnFunctionGroup" type="row_transformation_logic:ColumnFunctionGroup">
        <xsd:annotation>
          <xsd:documentation>The set of ColumnFunctions</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element ecore:resolveProxies="true" minOccurs="0" name="cubeLogic" type="cube_transformation_logic:FunctionalCubeLogic">
        <xsd:annotation>
          <xsd:documentation>The associated Cube Logic, which determines how actual rows are created and their linkage to source rows. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute ecore:unsettable="false" name="usedInSubsets" type="ecore:EBoolean">
      <xsd:annotation>
        <xsd:documentation>The CompareAttributeLineageModels Platform call in the platform_call package creates an AttributeLineageModel where items (such as BaseRowStructures and FunctionalRowLogics) which are in both models are marked with usedInSubset=true</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
</xsd:schema>
