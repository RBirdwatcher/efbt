<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns:bird_model="http://www.eclipse.org/bird_model" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:efbt_data_definition="http://www.eclipse.org/efbt_data_definition" xmlns:efbt_mapping="http://www.eclipse.org/efbt_mapping" xmlns:efbt_vtl_transformation="http://www.eclipse.org/efbt_vtl_transformation" xmlns:xsd="http://www.w3.org/2001/XMLSchema" ecore:nsPrefix="bird_model" ecore:package="bird_model" targetNamespace="http://www.eclipse.org/bird_model">
  <xsd:import namespace="http://www.eclipse.org/emf/2002/Ecore" schemaLocation="../../org.eclipse.emf.ecore/model/Ecore.xsd"/>
  <xsd:import namespace="http://www.eclipse.org/efbt_mapping" schemaLocation="efbt_mapping.xsd"/>
  <xsd:import namespace="http://www.eclipse.org/efbt_vtl_transformation" schemaLocation="efbt_vtl_transformation.xsd"/>
  <xsd:import namespace="http://www.eclipse.org/efbt_data_definition" schemaLocation="efbt_data_definition.xsd"/>
  <xsd:annotation>
    <xsd:documentation> This package describes the bird model 
 
 </xsd:documentation>
  </xsd:annotation>
  <xsd:annotation>
