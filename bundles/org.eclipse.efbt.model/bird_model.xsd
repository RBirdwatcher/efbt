 Contributors:
 Neil Mackenzie - initial API and implementation&#13;</xsd:appinfo>
  </xsd:annotation>
  <xsd:element ecore:ignore="true" name="BIRDModel" type="bird_model:BIRDModel"/>
  <xsd:complexType name="BIRDModel">
    <xsd:annotation>
      <xsd:documentation>This class stores the reference to all the BIRD artefacts that make up a release of BIRD</xsd:documentation>
    </xsd:annotation>
    <xsd:attribute ecore:reference="efbt_data_definition:DomainModule" name="domains">
      <xsd:annotation>
        <xsd:documentation>The Domains</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:list itemType="xsd:anyURI"/>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute ecore:reference="efbt_data_definition:MemberModule" name="members">
      <xsd:annotation>
        <xsd:documentation>The Members</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:list itemType="xsd:anyURI"/>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute ecore:reference="efbt_data_definition:VariableModule" name="variables">
      <xsd:annotation>
        <xsd:documentation>The Variables</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:list itemType="xsd:anyURI"/>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute ecore:reference="efbt_data_definition:CubeModule" name="cubes">
      <xsd:annotation>
        <xsd:documentation>The Cubes</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:list itemType="xsd:anyURI"/>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute ecore:reference="efbt_data_definition:CombinationModule" name="combinations">
      <xsd:annotation>
        <xsd:documentation>The Combinations</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:list itemType="xsd:anyURI"/>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute ecore:reference="efbt_vtl_transformation:TransformationSchemeModule" name="transformationSchemes">
      <xsd:annotation>
        <xsd:documentation>The Tranformation Schemes and contained Transformations</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:list itemType="xsd:anyURI"/>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute ecore:reference="efbt_mapping:MappingDefinitionModule" name="mappings">
      <xsd:annotation>
        <xsd:documentation>The Mappings</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:list itemType="xsd:anyURI"/>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute ecore:id="true" name="name" type="ecore:EString"/>
  </xsd:complexType>
</xsd:schema>
