<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns:core="http://www.eclipse.org/core" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:vtl_transformation="http://www.eclipse.org/vtl_transformation" xmlns:xsd="http://www.w3.org/2001/XMLSchema" ecore:nsPrefix="vtl_transformation" ecore:package="vtl_transformation" targetNamespace="http://www.eclipse.org/vtl_transformation">
