  <xsd:element ecore:ignore="true" name="TRANSFORMATION_SCHEME" type="vtl_transformation:TRANSFORMATION_SCHEME"/>
  <xsd:element ecore:ignore="true" name="TRANSFORMATION" type="vtl_transformation:TRANSFORMATION"/>
  <xsd:element ecore:ignore="true" name="TRANSFORMATION_NODE" type="vtl_transformation:TRANSFORMATION_NODE"/>
  <xsd:element ecore:ignore="true" name="NODE_RELATIONSHIP" type="vtl_transformation:NODE_RELATIONSHIP"/>
  <xsd:complexType ecore:name="TRANSFORMATION_SCHEME" name="TRANSFORMATION_SCHEME">
    <xsd:sequence>
      <xsd:element ecore:resolveProxies="true" maxOccurs="unbounded" minOccurs="0" name="tranformations" type="vtl_transformation:TRANSFORMATION"/>
    </xsd:sequence>
    <xsd:attribute ecore:id="true" ecore:name="transformation_scheme_id" name="transformation_scheme_id" type="ecore:EString"/>
    <xsd:attribute name="type" type="ecore:EString"/>
    <xsd:attribute name="description" type="ecore:EString"/>
    <xsd:attribute name="name" type="ecore:EString"/>
    <xsd:attribute name="code" type="ecore:EString"/>
    <xsd:attribute ecore:name="maintenance_agency_id" name="maintenance_agency_id" type="ecore:EString"/>
  </xsd:complexType>
  <xsd:complexType name="TRANSFORMATION">
    <xsd:sequence>
      <xsd:element ecore:resolveProxies="true" minOccurs="0" name="rootNode" type="vtl_transformation:TRANSFORMATION_NODE"/>
    </xsd:sequence>
    <xsd:attribute ecore:id="true" ecore:name="transformation_id" name="transformation_id" type="ecore:EString"/>
    <xsd:attribute name="description" type="ecore:EString"/>
    <xsd:attribute name="expression" type="ecore:EString"/>
    <xsd:attribute name="name" type="ecore:EString"/>
    <xsd:attribute name="code" type="ecore:EString"/>
    <xsd:attribute ecore:name="maintenance_agency_id" name="maintenance_agency_id" type="ecore:EString"/>
  </xsd:complexType>
  <xsd:complexType ecore:name="TRANSFORMATION_NODE" name="TRANSFORMATION_NODE">
    <xsd:sequence>
      <xsd:element ecore:resolveProxies="true" maxOccurs="unbounded" minOccurs="0" name="subNodes" type="vtl_transformation:TRANSFORMATION_NODE"/>
    </xsd:sequence>
    <xsd:attribute name="expresssion" type="ecore:EString"/>
    <xsd:attribute name="type" type="ecore:EString"/>
    <xsd:attribute ecore:reference="core:VARIABLE" name="variable" type="xsd:anyURI"/>
    <xsd:attribute ecore:reference="core:MEMBER" name="member" type="xsd:anyURI"/>
    <xsd:attribute ecore:id="true" ecore:name="transformation_node_id" name="transformation_node_id" type="ecore:EString"/>
    <xsd:attribute ecore:unsettable="false" name="level" type="ecore:EInt"/>
    <xsd:attribute ecore:unsettable="false" name="order" type="ecore:EInt"/>
    <xsd:attribute ecore:name="parent_node_id" name="parent_node_id" type="ecore:EString"/>
  </xsd:complexType>
  <xsd:complexType ecore:name="NODE_RELATIONSHIP" name="NODE_RELATIONSHIP">
    <xsd:attribute ecore:reference="vtl_transformation:TRANSFORMATION_NODE" name="sourceNode" type="xsd:anyURI"/>
    <xsd:attribute ecore:reference="vtl_transformation:TRANSFORMATION_NODE" name="targetNode" type="xsd:anyURI"/>
  </xsd:complexType>
</xsd:schema>
