  </xsd:annotation>
  <xsd:element ecore:ignore="true" name="MappingDefinitionModule" type="efbt_mapping:MappingDefinitionModule"/>
  <xsd:element ecore:ignore="true" name="CubeMappingModule" type="efbt_mapping:CubeMappingModule"/>
  <xsd:element ecore:ignore="true" name="MemberMappingModule" type="efbt_mapping:MemberMappingModule"/>
  <xsd:element ecore:ignore="true" name="VariableMappingModule" type="efbt_mapping:VariableMappingModule"/>
  <xsd:complexType name="MappingDefinitionModule">
    <xsd:annotation>
      <xsd:documentation>A Module containing SMCubes MappingDefintitions</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="module_management:Module">
        <xsd:sequence>
          <xsd:element ecore:resolveProxies="true" maxOccurs="unbounded" minOccurs="0" name="mappings" type="mapping:MAPPING_DEFINITION">
            <xsd:annotation>
              <xsd:documentation>The  MappingDefintitions</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CubeMappingModule">
    <xsd:annotation>
      <xsd:documentation>A Module containing SMCubes CubeMappings</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="module_management:Module">
        <xsd:sequence>
          <xsd:element ecore:resolveProxies="true" maxOccurs="unbounded" minOccurs="0" name="cubeMappings" type="mapping:CUBE_MAPPING">
            <xsd:annotation>
              <xsd:documentation>The CubeMappings</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="MemberMappingModule">
    <xsd:annotation>
      <xsd:documentation>A Module containing SMCubes MemberMappings</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="module_management:Module">
        <xsd:sequence>
          <xsd:element ecore:resolveProxies="true" maxOccurs="unbounded" minOccurs="0" name="memberMappings" type="mapping:MEMBER_MAPPING">
            <xsd:annotation>
              <xsd:documentation>The MemberMappings</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="VariableMappingModule">
    <xsd:annotation>
      <xsd:documentation>A Module containing SMCubes VariableMappings</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="module_management:Module">
        <xsd:sequence>
          <xsd:element ecore:resolveProxies="true" maxOccurs="unbounded" minOccurs="0" name="variableMappings" type="mapping:VARIABLE_MAPPING">
            <xsd:annotation>
              <xsd:documentation>The  VariableMappings</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
</xsd:schema>
