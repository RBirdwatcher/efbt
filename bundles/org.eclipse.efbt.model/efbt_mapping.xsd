<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:efbt_mapping="http://www.eclipse.org/efbt_mapping" xmlns:mapping="http://www.eclipse.org/mapping" xmlns:module_management="http://www.eclipse.org/module_management" xmlns:xsd="http://www.w3.org/2001/XMLSchema" ecore:nsPrefix="efbt_mapping" ecore:package="efbt_mapping" targetNamespace="http://www.eclipse.org/efbt_mapping">
  <xsd:import namespace="http://www.eclipse.org/mapping" schemaLocation="mapping.xsd"/>
  <xsd:import namespace="http://www.eclipse.org/module_management" schemaLocation="module_management.xsd"/>
  <xsd:annotation>
    <xsd:documentation>This Package contains the Modules comtaining SMCubes Mappings</xsd:documentation>
  </xsd:annotation>
