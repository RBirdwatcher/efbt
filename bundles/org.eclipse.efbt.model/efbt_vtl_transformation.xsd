  <xsd:element ecore:ignore="true" name="TransformationSchemeModule" type="efbt_vtl_transformation:TransformationSchemeModule"/>
  <xsd:element ecore:ignore="true" name="NodeRelationshipModule" type="efbt_vtl_transformation:NodeRelationshipModule"/>
  <xsd:complexType name="TransformationSchemeModule">
    <xsd:annotation>
      <xsd:documentation>A Module of Tranformation Schemes</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="module_management:Module">
        <xsd:sequence>
          <xsd:element ecore:resolveProxies="true" maxOccurs="unbounded" minOccurs="0" name="schemes" type="vtl_transformation:TRANSFORMATION_SCHEME">
            <xsd:annotation>
              <xsd:documentation>The TransformationSchemes</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="NodeRelationshipModule">
    <xsd:annotation>
      <xsd:documentation>A Module of Tranformation Node Relationships</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="module_management:Module">
        <xsd:sequence>
          <xsd:element ecore:resolveProxies="true" maxOccurs="unbounded" minOccurs="0" name="relationships" type="vtl_transformation:NODE_RELATIONSHIP">
            <xsd:annotation>
              <xsd:documentation>The Tranformation Node Relationships</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
</xsd:schema>
