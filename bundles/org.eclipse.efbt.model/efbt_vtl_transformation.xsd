<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:efbt_vtl_transformation="http://www.eclipse.org/efbt_vtl_transformation" xmlns:module_management="http://www.eclipse.org/module_management" xmlns:vtl_transformation="http://www.eclipse.org/vtl_transformation" xmlns:xsd="http://www.w3.org/2001/XMLSchema" ecore:nsPrefix="efbt_vtl_transformation" ecore:package="efbt_vtl_transformation" targetNamespace="http://www.eclipse.org/efbt_vtl_transformation">
  <xsd:import namespace="http://www.eclipse.org/vtl_transformation" schemaLocation="vtl_transformation.xsd"/>
  <xsd:import namespace="http://www.eclipse.org/module_management" schemaLocation="module_management.xsd"/>
  <xsd:annotation>
    <xsd:documentation>This Package contains modules for containing the SMCubes Tranformation related artefacts</xsd:documentation>
  </xsd:annotation>
